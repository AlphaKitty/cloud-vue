    "push": "cross-env NODE_ENV=prod report=false node build/build.js"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "bootstrap3": "^3.3.5",
    "core-js": "^3.8.3",
    "element-plus": "^2.1.9",
    "jquery": "^3.6.0",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.2.13",
    "babel-loader": "^8.2.5",
    "chalk": "^1.1.3",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "extract-text-webpack-plugin": "^3.0.2",
